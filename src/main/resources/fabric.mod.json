{
	"schemaVersion": 1,
	"id": "blackblock-perf",
	"version": "${version}",
	"name": "Blackblock Perf",
	"description": "Performance optimizations for Minecraft",
	"authors": [
		"Jelle De Loecker"
	],
	"contact": {
		"homepage": "https://blackblock.rocks/",
		"sources": "https://github.com/blackblock-rocks/blackblock-perf"
	},
	"license": "GPL3",
	"icon": "assets/blackblock/icon.png",
